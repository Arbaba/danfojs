    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
