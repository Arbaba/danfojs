{
  "name": "danfojs-node",
  "version": "0.2.1",
  "description": "JavaScript library providing high performance, intuitive, and easy to use data structures for manipulating and processing structured data.",
  "main": "dist/index.js",
  "types": "danfojs/types/index.d.ts",
  "directories": {
    "test": "danfojs/tests"
  },
  "contributors": [
    {
      "name": "Rising Odegua"
    },
    {
      "name": "Stephen Oni"
    }
  ],
  "files": [
    "dist/",
    "danfojs/src/",
    "danfojs/types"
  ],
  "dependencies": {
    "@tensorflow/tfjs-node": "^2.8.5",
    "frictionless.js": "0.13.4",
    "mathjs": "7.5.1",
    "stream-to-array": "^2.3.0",
    "table": "^5.4.6",
    "xlsx": "^0.16.7"
  },
  "scripts": {
    "test": "nyc mocha --require @babel/register danfojs/tests/*",
    "dev": "npm run lint && babel ./danfojs/src -d dist --no-comments",
    "build": "babel ./danfojs/src -d ./dist --no-comments",
    "lint": "eslint ./danfojs/src",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "patch": "npm version patch",
    "publish": "npm publish"
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "master"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensource9ja/danfojs.git"
  },
  "keywords": [
    "pandas",
    "data-analysis",
    "data-manipulation",
    "analysis"
  ],
  "author": "Rising Odegua <risingodegua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opensource9ja/danfojs/issues"
  },
  "homepage": "https://github.com/opensource9ja/danfojs#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.1",
    "@babel/runtime": "^7.12.13",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.1.0",
    "mocha": "^7.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
