{
  "name": "danfojs",
  "version": "0.0.1",
  "description": "Javascript version of Pandas. A library of data manipulation and analysis",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "danfojs/src/"
  ],
  "dependencies": {
