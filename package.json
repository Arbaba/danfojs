{
  "name": "panjs",
  "version": "0.0.1",
  "description": "Javascript version of Pandas. A library of data manipulation and analysis",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "panjs/src/"
  ],
  "dependencies": {
    "@tensorflow/tfjs": "^1.3.2"
  },
  "scripts": {
    "test": "nyc mocha --require @babel/register panjs/tests/*",
    "dev": "npm run lint && babel ./panjs/src -d dist --no-comments",
    "build": "babel ./panjs/src -d ./dist --no-comments",
    "lint": "eslint ./panjs/src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensource9ja/panjs.git"
  },
  "keywords": [
    "pandas",
    "data-analysis",
    "data-manipulation",
    "analysis"
  ],
  "author": "Rising Odegua <risingodegua@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opensource9ja/panjs/issues"
  },
  "homepage": "https://github.com/opensource9ja/panjs#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}